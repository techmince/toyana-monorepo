{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "auth/{**catch-all}"
        }
      },
      "ordering-route": {
        "ClusterId": "ordering-cluster",
        "AuthorizationPolicy": "Authenticated",
        "Match": {
          "Path": "bookings/{**catch-all}"
        }
      },
      "vendor-route": {
        "ClusterId": "vendor-cluster",
        "AuthorizationPolicy": "RequireVendor",
        "Match": {
          "Path": "vendors/{**catch-all}"
        }
      },
      "catalog-route": {
        "ClusterId": "catalog-cluster",
        "Match": {
          "Path": "catalog/{**catch-all}"
        }
      },
      "chat-route": {
        "ClusterId": "chat-cluster",
        "Match": {
          "Path": "chat/{**catch-all}"
        }
      },
      "admin-route": {
        "ClusterId": "admin-cluster",
        "AuthorizationPolicy": "Authenticated",
        "Match": {
          "Path": "admin/{**catch-all}"
        }
      },
      "alert-route": {
        "ClusterId": "alerts-cluster",
        "Match": {
          "Path": "alerts/{**catch-all}"
        }
      },
      "identity-openapi": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "auth/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/auth"
          }
        ]
      },
      "vendor-openapi": {
        "ClusterId": "vendor-cluster",
        "Match": {
          "Path": "vendors/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/vendors"
          }
        ]
      },
      "ordering-openapi": {
        "ClusterId": "ordering-cluster",
        "Match": {
          "Path": "bookings/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/bookings"
          }
        ]
      },
      "catalog-openapi": {
        "ClusterId": "catalog-cluster",
        "Match": {
          "Path": "catalog/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/catalog"
          }
        ]
      },
      "chat-openapi": {
        "ClusterId": "chat-cluster",
        "Match": {
          "Path": "chat/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/chat"
          }
        ]
      },
      "admin-openapi": {
        "ClusterId": "admin-cluster",
        "Match": {
          "Path": "admin/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/admin"
          }
        ]
      }
    },
    "Clusters": {
      "alerts-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://"
          }
        }
      },
      "identity-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://identity-api:8080"
          }
        }
      },
      "ordering-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://ordering-api:8080"
          }
        }
      },
      "vendor-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://vendor-api:8080"
          }
        }
      },
      "catalog-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://catalog-api:8080"
          }
        }
      },
      "chat-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://chat-api:8080"
          }
        }
      },
      "admin-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://admin-api:8080"
          }
        }
      }
    }
  },
  "Jwt": {
    "Issuer": "Toyana",
    "Audience": "Toyana",
    "Key": "ThisIsASecretKeyForToyanaProjectAndItMustBeLongEnough"
  }
}